BusType         = 8		; Pcmcia bus
AddReg          = AddReg.interfaces, AddReg.Params, SLA4111.AddReg, Symbol.reg

;(T3)
[SLA4100.ndi.NT]
Characteristics = 0x84	; NCF_HAS_UI, NCF_PHYSICAL
PromptForESS_ID = 1
BusType         = 8		; Pcmcia bus
AddReg          = AddReg.interfaces, AddReg.Params, SLA4100.AddReg, Symbol.reg

[SLA4111.AddReg]
