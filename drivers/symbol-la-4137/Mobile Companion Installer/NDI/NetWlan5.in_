;(Windows98/ME)
[SLA41111.reg]
HKR,	,DriverDesc,	, %SLA4111_DriverDesc%
HKR,	,CLSGUID,		, %SYMBOL_DS11_GUID%
HKR,Ndi	,DeviceID,		, "PCMCIA\Symbol_Technologies-LA4111_Spectrum24_Wireless_LAN_PC_Card-077d"
HKR,	,AdapterModel, 0, %SLA4111_AdapterModel%
HKR,	,Provider,	   0, %Symbol_Provider%	
HKR,	,ESQUARE,	   0, "0"

[SLA41100.reg]
HKR,	,DriverDesc,	, %SLA4100_DriverDesc%
HKR,	,CLSGUID,		, %SYMBOL_DS11_T3_GUID%
HKR,Ndi	,DeviceID,		, "PCMCIA\Symbol-Spectrum24_LA4100_Series_WLAN_PC_Card-e063"
HKR,	,AdapterModel, 0, %SLA4100_AdapterModel%
HKR,	,Provider,	   0, %Symbol_Provider%	
HKR,	,ESQUARE,	   0, "1"

[Symbol.reg]
;


