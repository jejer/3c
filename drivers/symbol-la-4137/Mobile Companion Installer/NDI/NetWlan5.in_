; Wrapper 
;
[AddReg.interfaces]
HKR, Ndi,Service,				0, "NetWlan5"
HKR, Ndi\Interfaces, UpperRange,0, "ndis5"
HKR, Ndi\Interfaces, LowerRange,0, "ethernet"

[AddReg.XP.interfaces]
HKR, Ndi,Service,				0, "NetWlanXP"
HKR, Ndi\Interfaces, UpperRange,0, "ndis5"
HKR, Ndi\Interfaces, LowerRange,0, "ethernet"

;-------------------------------------------------------------------------------
; Service and Driver
;
[NetWlan5.Service]
DisplayName    = %NetWlan5.ServiceDesc%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\NetWlan5.sys
LoadOrderGroup = NDIS

[NetWlan5.Eventlog]
Addreg = NetWlan5.Eventlog.reg

[NetWlan5.Eventlog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll;%%SystemRoot%%\System32\Drivers\NetWlan5.sys"
HKR, , TypesSupported,   0x00010001, 7

[NetWlanXP.Service]
DisplayName    = %NetWlanXP.ServiceDesc%
ServiceType    = 1
StartType      = 3
ErrorControl   = 1
ServiceBinary  = %12%\NetWlanXP.sys
LoadOrderGroup = NDIS

[NetWlanXP.Eventlog]
Addreg = NetWlanXP.Eventlog.reg

[NetWlanXP.Eventlog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll;%%SystemRoot%%\System32\Drivers\NetWlanXP.sys"
HKR, , TypesSupported,   0x00010001, 7

