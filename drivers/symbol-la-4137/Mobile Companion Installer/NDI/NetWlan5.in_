[SLA4111.ndi]
AddReg		= ndi.reg, SLA41111.reg, Common.PCMCIA.reg, Symbol.reg

[SLA4100.ndi]
AddReg		= ndi.reg, SLA41100.reg, Common.PCMCIA.reg, Symbol.reg

