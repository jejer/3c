;--------------------------------------------------------------------------------
; Symbol PCMCIA Card (WinXP)
;
;(T2)
[XP_SLA4111.ndi.NT]
Characteristics = 0x84	; NCF_HAS_UI, NCF_PHYSICAL
PromptForESS_ID = 1		; Magic Setting to force the CoInstaller to prompt for ESS Id
BusType         = 8		; Pcmcia bus
AddReg          = AddReg.XP.interfaces, AddReg.XP.Params, SLA4111.AddReg, Symbol.reg

;(T3)
[XP_SLA4100.ndi.NT]
Characteristics = 0x84	; NCF_HAS_UI, NCF_PHYSICAL
PromptForESS_ID = 1
BusType         = 8		; Pcmcia bus
AddReg          = AddReg.XP.interfaces, AddReg.XP.Params, SLA4100.AddReg, Symbol.reg

[XP_SLA4111.ndi.NT.Services]
AddService = NetWlanXP, 2, NetWlanXP.Service, NetWlanXP.EventLog

[XP_SLA4100.ndi.NT.Services]
AddService = NetWlanXP, 2, NetWlanXP.Service, NetWlanXP.EventLog

