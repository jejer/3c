HKR,Ndi	,DeviceID,		0, "PCI\VEN_1562&DEV_0001"
HKR,	,AdapterModel,  0, %SLA4113_AdapterModel%
HKR,	,Provider,	    0, %Symbol_Provider%	
HKR,	,ESQUARE,		0, "0"


