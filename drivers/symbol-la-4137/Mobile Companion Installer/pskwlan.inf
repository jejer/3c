;-------------------------------------------------------------------------
; PSKWLAN.INF -- Sample NDIS Protocol Driver
;
; Copyright (c) 2003, Microsoft Corporation
;-------------------------------------------------------------------------
[version]
Signature   = "$Windows NT$"
Class       = NetTrans
ClassGUID   = {4d36e975-e325-11ce-bfc1-08002be10318}
Provider    = %Symbol%
LayoutFile  = layout.inf

[Manufacturer]
%Symbol%=SYMBOL

[SYMBOL]
%PSKWLAN_Desc%=Install, SYMBOL_PSKWLAN

;-------------------------------------------------------------------------
; Installation Section
;-------------------------------------------------------------------------
[Install]
AddReg=Inst_Ndi
Characteristics=0x0 ; 
;CopyFiles=CpyFiles_Sys

;-------------------------------------------------------------------------
; Ndi installation support
;-------------------------------------------------------------------------
[Inst_Ndi]
HKR,Ndi,Service,,"PskWLAN"
HKR,Ndi,HelpText,,%PSKWLAN_HelpText%
HKR, Ndi\Interfaces, UpperRange,, noupper
HKR,"Ndi\Interfaces","LowerRange",,"ndis5,ndis4,ndis5_prot"

;-------------------------------------------------------------------------
; Service installation support
;-------------------------------------------------------------------------
[Install.Services]
AddService=PskWLAN,,PSKWLAN_Service_Inst

[PSKWLAN_Service_Inst]
DisplayName     = %PSKWLAN_Desc%
ServiceType     = 1 ;SERVICE_KERNEL_DRIVER
StartType       = 2 ;SERVICE_AUTO_START
ErrorControl    = 1 ;SERVICE_ERROR_NORMAL
ServiceBinary   = %12%\pskwlan.sys
LoadOrderGroup  = NDIS
Description     = %PSKWLAN_Desc%

[Install.Remove.Services]
DelService=PskWLAN,0x200

;-------------------------------------------------------------------------
; Declare Destination Directories for file copy/deletion
;-------------------------------------------------------------------------
[DestinationDirs]
CpyFiles_Sys    = 12    ; DIRID_DRIVERS


;-------------------------------------------------------------------------
; Files to Copy/Delete - Referenced by Install and Remove sections above
;-------------------------------------------------------------------------
[CpyFiles_Sys]
pskwlan.sys,,,2

[Strings]
Symbol = "Symbol Technologies"
PSKWLAN_Desc = "Symbol Technologies Pre-Shared Key (PSK) Protocol for WPA"
PSKWLAN_HelpText = "This protocol supports PSK Key Management under WPA"





